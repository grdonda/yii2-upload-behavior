{
  "name": "grdonda/yii2-upload-behavior",
  "type": "yii2-extension",
  "description": "Yii2 file/image upload behavior for ActiveRecord",
  "keywords": [
    "file",
    "image",
    "upload",
    "behavior",
    "resize",
    "thumb"
  ],
  "license": "MIT",
  "homepage": "http://yiidreamteam.com/yii2/upload-behavior",
  "authors": [
    {
      "name": "Alexey Samoylov",
      "email": "alexey.samoylov@gmail.com",
      "homepage": "http://yiidreamteam.com/team/alexey",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2.0",
    "yiisoft/yii2": "~2.0",
    "grdonda/phpthumb": "~2.3"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/grdonda/phpthumb"
    }
  ],
  "autoload": {
    "psr-4": {
      "yiidreamteam\\upload\\": "./src"
    }
  }
}
